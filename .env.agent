# إعدادات الوكيل الذكي

# مزود الخدمة: "groq" أو "ollama"
AI_PROVIDER=groq

# Groq API Configuration
# احصل على المفتاح من https://console.groq.com/
GROQ_API_KEY=your_groq_api_key_here
GROQ_MODEL=mixtral-8x7b-32768

# Ollama Configuration (إذا كنت تستخدم Ollama محليًا)
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama2

# إعدادات النموذج
MAX_TOKENS=2048
TEMPERATURE=0.7

# وضع التصحيح
DEBUG=False
